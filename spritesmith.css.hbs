{{#sprites}}
// {{name}} 图片:
${{name}}-width: ({{width}}rem/100);
${{name}}-height: ({{height}}rem/100);
${{name}}-bg-image: url(../img/{{escaped_image}});
${{name}}-bg-position: ({{offset_x}}rem/100) ({{offset_y}}rem/100);
${{name}}-bg-size: ({{total_width}}rem/100) ({{total_height}}rem/100);

{{/sprites}}
